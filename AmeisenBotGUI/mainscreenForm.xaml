<Window x:Name="mainscreen" x:Class="AmeisenBotGUI.mainscreenForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AmeisenBotGUI"
        mc:Ignorable="d"
        Title="AmeisenBot - " Height="350" Width="500" Loaded="mainscreen_Loaded" Background="#FF303030" WindowStyle="None" ResizeMode="NoResize" MouseDown="mainscreen_MouseDown">
    <Grid>
        <Rectangle Stroke="#FFFFA200" StrokeThickness="2" Margin="-2,0"/>
        <ProgressBar x:Name="progressBarHPTarget" HorizontalAlignment="Left" Height="26" Margin="200,41,0,0" VerticalAlignment="Top" Width="164" Foreground="#FFFF6060" FontFamily="Consolas" BorderBrush="#FFFFA200" Background="{x:Null}" BorderThickness="2"/>
        <ProgressBar x:Name="progressBarEnergyTarget" HorizontalAlignment="Left" Height="26" Margin="200,72,0,0" VerticalAlignment="Top" Width="164" Foreground="#FFA0FFFF" Background="{x:Null}" FontFamily="Consolas" BorderBrush="#FFFFA200" BorderThickness="2"/>
        <ProgressBar x:Name="progressBarHP" HorizontalAlignment="Left" Height="26" Margin="10,41,0,0" VerticalAlignment="Top" Width="164" Foreground="#FFFF6060" FontFamily="Consolas" Background="{x:Null}" BorderBrush="#FFFFA200" BorderThickness="2"/>
        <ProgressBar x:Name="progressBarEnergy" HorizontalAlignment="Left" Height="26" Margin="10,72,0,0" VerticalAlignment="Top" Width="164" Foreground="#FFA0FFFF" Background="{x:Null}" FontFamily="Consolas" BorderBrush="#FFFFA200" BorderThickness="2"/>
        <ProgressBar x:Name="progressBarXP" HorizontalAlignment="Left" Height="26" Margin="10,103,0,0" VerticalAlignment="Top" Width="164" Foreground="#FFFFA0FB" Background="{x:Null}" FontFamily="Consolas" BorderBrush="#FFFFA200" BorderThickness="2"/>
        <Label x:Name="labelName" Content="..." HorizontalAlignment="Left" VerticalAlignment="Top" Margin="2,10,0,0" FontWeight="Bold" FontFamily="Consolas" FontSize="16" Foreground="White"/>
        <Label x:Name="labelNameTarget" Content="..." HorizontalAlignment="Left" VerticalAlignment="Top" Margin="192,10,0,0" FontWeight="Bold" FontFamily="Consolas" FontSize="16" Foreground="White"/>
        <Label x:Name="labelHP" Content="" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" FontFamily="Consolas"/>
        <Label x:Name="labelHPTarget" Content="" HorizontalAlignment="Left" Margin="200,41,0,0" VerticalAlignment="Top" FontFamily="Consolas"/>
        <Label x:Name="labelEnergy" Content="" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top" FontFamily="Consolas"/>
        <Label x:Name="labelEnergyTarget" Content="" HorizontalAlignment="Left" Margin="200,72,0,0" VerticalAlignment="Top" FontFamily="Consolas"/>
        <Label x:Name="labelXP" Content="" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top" FontFamily="Consolas"/>
        <Label x:Name="labelPosition" Content="X: Y: Z: R:" HorizontalAlignment="Left" Margin="6,134,0,0" VerticalAlignment="Top" FontFamily="Consolas" Foreground="White"/>
        <Label x:Name="labelPositionTarget" Content="X: Y: Z: R:" HorizontalAlignment="Left" Margin="196,134,0,0" VerticalAlignment="Top" FontFamily="Consolas" Foreground="White"/>
        <Label x:Name="labelDebugInfo" Content="- DebugInfo -" HorizontalAlignment="Left" Margin="10,235,0,0" VerticalAlignment="Top" FontFamily="Consolas" Foreground="White"/>
        <Label x:Name="labelDebugInfoTarget" Content="- DebugInfo -" HorizontalAlignment="Left" Margin="200,235,0,0" VerticalAlignment="Top" FontFamily="Consolas" Foreground="White"/>
        <Button x:Name="buttonMoveToTarget" Content="Move to Target" Margin="0,0,10,10" Click="buttonMoveToTarget_Click" Height="30" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="100" BorderThickness="2" BorderBrush="#FFFFA200" Background="{x:Null}" Foreground="White"/>
        <Button Content="X" Margin="0,0,-2,0" VerticalAlignment="Top" FontFamily="Consolas" Click="Button_Click" FontWeight="Bold" Background="{x:Null}" BorderBrush="#FFFFA200" Foreground="White" BorderThickness="2" HorizontalAlignment="Right" Width="20"/>
        <Label x:Name="labelDistance" Content="Distance:" HorizontalAlignment="Left" Margin="194,103,0,0" VerticalAlignment="Top" Foreground="White" Background="{x:Null}"/>
    </Grid>
</Window>
